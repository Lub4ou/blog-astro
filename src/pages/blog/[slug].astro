---
import type { GetStaticPaths } from "astro";
import {articlesList} from "../../quearies/articles.js"
import { Image } from "astro:assets";

export const getStaticPaths = (async () => {
  return articlesList.posts.data.map(({ attributes }) => ({
    params: { slug: attributes.slug },
    props: attributes
  }));
}) satisfies GetStaticPaths;

const articleData = Astro.props
---


<Image src={articleData.image.data.attributes.url} alt={articleData.image.data.attributes.alternativeText || "altvbgh"} width="200" height="200" ></Image>


<h1>{articleData.name}</h1>